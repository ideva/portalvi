<br/><h1>Data Laporan <%= @nama_laporan  %> </h1>

<table>
  <tr>
    <th>Hari/Tanggal</th>
    <th>Jumlah Pasien</th>
    <th>Pemeriksaan (Rp)</th>
    <th>Penunjang Diagnostik (Rp)</th>
    <th>Tindakan Medis (Rp)</th>
    <th>Kantong Darah (Rp)</th>
    <th>Haemodialisa (Rp)</th>
    <th>Yan Obat (Rp)</th>
    <th>Total (Rp)</th>
  </tr>

  <% @laporans.each do |laporan| %>
  <tr>
    <td><%= laporan["Tanggal"] %></td>
    <td><%= laporan["Jumlah Pasien"] %></td>
    <td><%= laporan["Pemeriksaan"] %></td>
    <td><%= laporan["Penunjang Diagnostik"] %></td>
    <td><%= laporan["Tindakan Medis"] %></td>
    <td><%= laporan["Kantong Darah"] %></td>
    <td><%= laporan["Haemodialisa"] %></td>
    <td><%= laporan["Yan Obat"] %></td>
    <td><%= laporan["Total"] %></td>
  </tr>

<% end %>

</table>
