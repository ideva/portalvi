<% @tindakan_medis.each do |tindakan_medi| %>
  <tr>
    <td><%= tindakan_medi.kode %></td>
    <td><%= tindakan_medi.kategori_tindakan_medi.nama rescue "-" %></td>
    <td><%= tindakan_medi.nama %></td>
    <td><%= tindakan_medi.tarif %></td>
    <td><%= tindakan_medi.status_flag %></td>
    <td><%= link_to 'Ubah/Hapus', edit_tindakan_medi_path(tindakan_medi) %></td>
  </tr>
<% end %>

 <tr style="background-color: transparent">
    <td colspan="100">
      <div class="digg_pagination">
        <div class="page_info">
          <%= raw page_entries_info @tindakan_medis %>
        </div>
        <%= will_paginate @tindakan_medis, :class => 'pagination ajax' %>
      </div>
    </td>
  </tr>
  <script type="text/javascript">
    ajaxifyPagination();
  </script>